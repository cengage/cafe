{
  "name": "@cafe/cafe-client-demo",
  "version": "1.1.2",
  "main": "index.js",
  "scripts": {
    "prepublish": "echo No prepublish steps required",
    "prepare": "npm run build",
  
    "maintainer-clean": "run-s maintainer-clean:*",
    "maintainer-clean:package-lock": "rimraf package-lock.json",
    "maintainer-clean:clean": "npm run clean",
    "maintainer-clean:node": "rimraf node_modules",
  
    "clean": "run-s clean:*",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf tmp",
    "clean:coverage": "rimraf coverage",
    "clean:env": "rimraf src/environments/version.ts",

    "generate": "run-p generate:*",
    "generate:versions": "cafe-version --path .",

    "build": "run-s build:*",
    "build:clean": "npm run clean:dist",
    "build:generate": "npm run generate",
    "build:build": "npm run ng:build",
    "build:dist": "npm run dist",

    "dist": "run-s dist:*",
    "no:dist:clean": "rimraf dist",
    "no:dist:prepare": "mkdirp dist",
    "no:dist:copy-source": "ncp src dist",
    "no:dist:copy-js": "cp *.js dist",
    "dist:copy-package": "cp package.json dist",

    "deploy": "run-s deploy:*",
    "deploy:build": "npm run build",
    "deploy:dist": "npm run dist",
    "deploy:publish": "npm publish dist",

    "postinstall": "echo No postinstall tasks",

    "start": "run-s start:*",
    "start:generate": "npm run generate",
    "start:serve": "npm run ng:start",

    "e2e": "run-s e2e:test:* --continue-on-error",
    "e2e:test:build": "npm run build",
    "e2e:test:start": "npm run e2e:background:start",
    "e2e:test:run": "npm run cypress:run",
    "e2e:test:terminate": "npm run e2e:background:terminate",

    "e2e:start": "run-s e2e:start:*",
    "e2e:start:build": "npm run build",
    "e2e:start:serve": "ng serve --watch false --live-reload false",
    "e2e:background:start": "ng serve --watch false --live-reload false &",
    "e2e:background:terminate": "echo \"killing $(lsof -t -i:4200)\" && kill $(lsof -t -i:4200)",

    "test": "run-s test:*",
    "test:generate": "npm run generate",
    "test:test": "npm run ng:test:coverage",

    "jenkins:e2e": "run-s jenkins:e2e:*",
    "jenkins:e2e:generate": "npm run generate",
    "jenkins:e2e:test": "npm run cypress:jenkins",

    "tsc:build": "tsc --build tsconfig.json",
    "tsc:build.trace": "tsc --traceResolution --build tsconfig.json",

    "ng:build": "ng build",
    "ng:start": "ng serve",
    "ng:test:test": "ng test",
    "ng:test:watch": "ng test --watch",
    "ng:test:coverage": "ng test --code-coverage",

    "lint": "ng lint",

    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:jenkins": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=tmp/e2e-test-output.xml,toConsole=true'"
  },
  "oldscripts": {
    "old:prepublish": "echo No prepublish steps required",
    "old:prepare": "npm run build",
    "old:tsc": "tsc --build tsconfig.json",
    "old:ng": "ng",
    "old:build": "npm run generate_code && npm run tsc",
    "old:oldbuild": "npm run generate_code && ng build",
    "old:build:library": "npm run generate_code  && ng build",
    "old:start": "npm run generate_code && ng serve",
    "old:start_for_e2e": "npm run generate_code && ng serve --watch false --live-reload false",
    "old:start_for_e2e:background": "ng serve --watch false --live-reload false &",
    "old:test": "npm run generate_code && ng test",
    "old:lint": "ng lint",
    "old:e2e": "npm run generate_code && cypress run",
    "old:e2e:jenkins": "npm run generate_code && cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=tmp/e2e-test-output.xml,toConsole=true'",
    "old:generate_versions": "node node_modules/@cafe/cafe-build-utilities/version.js --path .",
    "old:generate_code": "npm run generate_versions",
    "old:test:watch": "npm run generate_code && ng test --watch",
    "old:test:coverage": "npm run generate_code && ng test --code-coverage",
    "old:e2e:open": "npm run generate_code && cypress open",
    "old:e2e:start-and-execute": "npm-run-all build:library start_for_e2e:background e2e terminate_server --continue-on-error",
    "old:terminate_server": "echo \"killing $(lsof -t -i:4200)\" && kill $(lsof -t -i:4200)",
    "old:deploy": "npm run generate_code && npm publish dist/cafe-version-page",
    "old:postinstall": "npm run generate_code",
    "old:clean": "rm -rf node_modules dist tmp coverage",
    "deploy": "npm run build && npm publish"
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
